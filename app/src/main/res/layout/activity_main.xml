<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.santhoshkumar.registerwithmail.MainActivity">

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:inputType="none"
        android:text="0123456789"
        android:ems="10"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:id="@+id/tnsid"
        android:layout_margin="10dp"
        android:padding="10dp" />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:inputType="none"
        android:text="sandy"
        android:ems="10"
        android:id="@+id/uname"
        android:layout_below="@+id/tnsid"
        android:layout_margin="10dp"
        android:padding="10dp" />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:inputType="none"
        android:text="Salem"
        android:ems="10"
        android:layout_below="@+id/uname"
        android:layout_alignParentStart="true"
        android:id="@+id/addr"
        android:layout_margin="10dp"
        android:padding="10dp" />

    <EditText
        android:layout_height="wrap_content"
        android:inputType="none"
        android:text="Book1, Book2"
        android:ems="10"
        android:id="@+id/bname"
        android:layout_width="match_parent"
        android:layout_below="@+id/no"
        android:layout_margin="10dp"
        android:padding="10dp" />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:inputType="none"
        android:text="9487735306"
        android:ems="10"
        android:id="@+id/no"
        android:layout_below="@+id/addr"
        android:layout_alignParentStart="true"
        android:layout_margin="10dp"
        android:padding="10dp" />

    <Button
        android:text="Submit"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="29dp"
        android:id="@+id/button"
        android:layout_below="@+id/bname"
        android:layout_alignParentStart="true" />
</RelativeLayout>
